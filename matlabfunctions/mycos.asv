function [y,T] = mycos(f0, dur, fs)
%f0: frequency in Hz
%dur: duration in seconds
%fs: sampling rate
%returns y: Vector of sampling points that form a sinusoid
%        T: Vector of time that has been discreetised

w = 2*pi*f0;

T = [0:dur/fs:dur];
%T = [0:dur/fs:3/200];

y = cos(w*T);
